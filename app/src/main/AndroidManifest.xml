<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ali.iqbreaker3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ali.iqbreaker3.MainActivity" android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ali.iqbreaker3.LoginActivity" />
        <activity android:name="com.example.ali.iqbreaker3.ProfileActivity" />
        <activity android:name="com.example.ali.iqbreaker3.RegistrationActivity" />
        <activity android:name="com.example.ali.iqbreaker3.HighestScoreActivity" />
        <activity android:name="com.example.ali.iqbreaker3.QuizActivity" />
        <activity android:name="com.example.ali.iqbreaker3.FactsActivity" />
        <activity android:name="com.example.ali.iqbreaker3.UserPrefActivity"></activity>
    </application>

</manifest>